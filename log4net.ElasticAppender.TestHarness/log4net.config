<log4net>
  <appender name="Console" type="log4net.Appender.ConsoleAppender">
<!--    <layout type="log4net.Layout.PatternLayout">-->
<!--      <conversionPattern value="%-4timestamp [%thread] %-5level %logger %ndc - %message%newline" />-->
<!--    </layout>-->
    <layout type="log4net.ElasticSearch.Layout.JsonLayout, log4net.ElasticSearch" />
  </appender>
  <appender name="Elastic" type="log4net.ElasticSearch.Appender.ElasticSearchAppender, log4net.ElasticSearch">
    <Host value="localhost" />
    <Port value="9200" />
    <Index value="onlineclaims" />
    <DocumentType value="applogs" />
    <layout type="log4net.ElasticSearch.Layout.JsonLayout, log4net.ElasticSearch" />
  </appender>
  <root>
    <level value="ALL" />
    <appender-ref ref="Console" />
    <appender-ref ref="Elastic" />
  </root>
</log4net>